<div class="container">
  <div class="row">
    <div class="col">
      <h1>Add a list</h1>

      <%= simple_form_for @list do |f| %>
        <%= f.input :name %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit , class:"btn btn-primary" %>
      <% end %>

    <%= link_to "Back to lists", lists_path %>
    </div>
  </div>
</div>
